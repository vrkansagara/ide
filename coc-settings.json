{
	"intelephense.environment.documentRoot": "index.php",
	"intelephense.enable": true,
	"intelephense.maxMemory": 512,
	"intelephense.progress.enable": true,
	"intelephense.diagnostics.enable": false,
	"intelephense.completion.maxItems": 25,
	"intelephense.completion.insertUseDeclaration": true,
	"psalm.trace.server": "messages",
	"psalm.enable": true,
	"psalm.configPaths": ["~/.vim/psalm.xml", "psalm.xml", "psalm.xml.dist"],
	"psalm.phpExecutablePath": "~/.vim/vendor/bin/psalm-language-server",
	"coc.preferences.jumpCommand": "tabe",
	"sh.enable": true,
	"snippets.snipmate.enable": true,
	"snippets.snipmate.author": "Vallabh Kansagara",
	"snippets.ultisnips.directories": ["~/.vim/src/UltiSnips"],
	"snippets.userSnippetsDirectory": "~/.vim/src/UltiSnips",
	"snippets.extends": {
		"php": ["phtml", "php"],
		"cpp": ["c", "h","cpp"],
		"javascriptreact": ["javascript"],
		"typescript": ["javascript", "js","ts"]
	},
	"clangd.enabled": true
}
